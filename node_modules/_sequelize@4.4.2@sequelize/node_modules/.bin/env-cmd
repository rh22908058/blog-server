#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_env-cmd@5.1.0@env-cmd/bin/env-cmd.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_env-cmd@5.1.0@env-cmd/bin/env-cmd.js" "$@"
  ret=$?
fi
exit $ret
